<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiSwitchBtn">
        <attr name="strokeRadius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="indicatorWidth" format="dimension" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="selectedTab" format="integer" />
        <attr name="selectedColor" format="color|reference" />
        <attr name="unselectedColor" format="color|reference" />
        <attr name="selectedTextColor" format="color|reference" />
        <attr name="unselectedTextColor" format="color|reference" />
        <attr name="switchTabs" format="reference" />
        <attr name="typeface" format="string" />
        <attr name="disableColor" format="color|reference" />
        <attr name="disableTextColor" format="color|reference" />
        <attr name="blockStyle">
            <enum name="original" value="0" />
            <enum name="brim" value="1" />
        </attr>
    </declare-styleable>
</resources>